@model IEnumerable<Application.Web.Models.HeroViewModel>
@{
    ViewBag.Title = "Home Page";
}

<div class="row">
    <div class="container well" style="margin-top:10px;background: #374c60;">
        <div class="col-md-6">
            <h1>Diablo III Ladders</h1>
            <p>
                Start by entering your battletag on top left and press 'Import Now' on next page.
                Leaderboards and rankings for Power(Hero Power), Burst DPS and EHP(Toughness) are calculated based off items unbuffed (Burst DPS takes best Elemental% * Elite% * Skill%). Season, paragon, build, clan and legendary leaderboards are also available and up to date DPS and toughness calculators found in every character sheet.
            </p>
        </div>
        <div class="col-md-6">
            Latest Guides:
            <ul class="nav nav-tabs">
                <li class="active"><a href="#home" data-toggle="tab" aria-expanded="true">All</a></li>
                <li class=""><a href="#profile" data-toggle="tab" aria-expanded="false">Barbarian</a></li>
                <li class=""><a href="#profile" data-toggle="tab" aria-expanded="false">Wizard</a></li>
                <li class=""><a href="#profile" data-toggle="tab" aria-expanded="false">DH</a></li>
