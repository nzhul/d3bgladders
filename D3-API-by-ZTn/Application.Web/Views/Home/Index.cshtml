                                <th>Battle Tag</th>
                                <th>Hero</th>
                                <th>Paragon</th>
                                <th>Class</th>
                                <th>Power</th>
                            </tr>
                        </thead>
                        <tbody>
                            @for (int i = 0; i < Model.Count(); i++)
                            {
                                var hero = Model.ElementAt(i);
                                <tr>
                                    <td>@hero.ID</td>
                                    <td>@hero.BattleTag</td>
                                    <td>@hero.Name</td>
                                    <td>@hero.ParagonLevel</td>
                                    <td>@hero.HeroClass</td>
                                    <td>@hero.Damage</td>
                                </tr>
                            }
